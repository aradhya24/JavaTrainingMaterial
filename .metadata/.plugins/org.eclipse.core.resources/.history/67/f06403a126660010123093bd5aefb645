package facadeassignment.tictactoe.model;

public class Board { 
	
	char[][] grid;
	
	public void displayBoard() {
		System.out.println("Current Board: ");
		for(int i = 0 ; i < 4 ; i++) {
			System.out.println();
			for(int j = 0 ; j < 4 ; j++) {
				System.out.print(grid[i][j] + " ");
			}
		}
	}
	
		
	

}
