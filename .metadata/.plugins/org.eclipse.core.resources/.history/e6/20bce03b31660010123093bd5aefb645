package facadeassignment.tictactoe.model;

public class Player {
	
    public Player(String name, Character symbol) {
		super();
		this.name = name;
		this.symbol = symbol;
	}
	private String name;
    private Character symbol;
    
    
	public String getName() {
		return name;
	}
	public Character getSymbol() {
		return symbol;
	}
    
    
}
