package ISP.solution.worker;

public class Loader implements IWorker {

	@Override
	public void eat() {
		System.out.println("loader Started eating....");
	}

	@Override
	public void rest() {
		System.out.println("loader is Resting..");
	}

	@Override
	public void start() {
		System.out.println("loader Started Working....");
	}

	@Override
	public void stop() {
		System.out.println("loader stopped ....");
	}

}
